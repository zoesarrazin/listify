  <% end %>

  <nav>
  <ul class="pager">
  
<a href="/lists/<%= @list.id %>/edit">Edit</a> |
<a href="/lists/<%= @list.id %>/destroy">Destroy</a>
  </ul>
</nav>

  
 
 