<h1>User #<%= @user.id %></h1>

<div>
  <strong>Name</strong>:<br />
  <p><%= @user.name %></p>
</div>
<div>
  <strong>Email</strong>:<br />
  <p><%= @user.email %></p>
</div>
<div>
  <strong>Password</strong>:<br />
  <p><%= @user.password %></p>
</div>
<div>
  <strong>Password digest</strong>:<br />
  <p><%= @user.password_digest %></p>
</div>

<a href="/users/<%= @user.id %>/edit">Edit</a> |
<a href="/users/<%= @user.id %>/destroy">Destroy</a>
